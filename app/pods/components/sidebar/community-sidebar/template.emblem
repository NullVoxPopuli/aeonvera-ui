li
  .text-center
    img src='{{model.logoUrlThumb}}'
    h5
      link-to 'my-communities.manage' model.id
        = model.name
li.divider
li.show-for-medium-up View
li.show-for-small-only.white-color.margin-top-10 View
li: link-to 'my-communities.manage.registrations'
    span Registrations
li: link-to 'register' model.domain
    span Registration Form

li: link-to 'my-communities.manage.embed-registration'
    span Embed Code

li.divider
li.show-for-medium-up Reporting
li.show-for-small-only.white-color.margin-top-10 Reporting
li: link-to 'my-communities.manage.revenue'
    span Revenue

li: link-to 'my-communities.manage.membership'
    span Membership

li.divider
li.show-for-medium-up Edit
li.show-for-small-only.white-color.margin-top-10 Edit
li: link-to 'my-communities.manage.lessons'
    span Lessons
li: link-to 'my-communities.manage.dances'
    span Dances

li: link-to 'my-communities.manage.payment-processors'
    span Payment Proccessing
li: link-to 'my-communities.manage.collaboration'
    span Collaborators

li.divider

li
  link-to 'my-communities.manage.edit' model.id classNames='button expand'
    span Edit Community Settings
