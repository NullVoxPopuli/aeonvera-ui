{{#paper-menu isOpen=isOpen as |menu|}}
  {{#menu.trigger}}
    {{#paper-button onClick=null iconButton=true}}
      <i class='fa fa-ellipsis-v md-menu-origin'></i>
    {{/paper-button}}
  {{/menu.trigger}}

  {{#menu.content width=width as |content|}}
    {{yield content menu}}
  {{/menu.content}}
{{/paper-menu}}
